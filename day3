#include <stdio.h>
#include <stdlib.h>

int read ()
{
    int i;
printf("enter a integer\n");
scanf("%d",&i);
return i;
}
int execute(int a,int b)
{    int sum;
     sum=a+b;
    return(sum);
}
void output(int sum)
{
  printf("%d",sum);
}
int main()
{
    int a,b,sum;
    a=read();
    b=read();
    sum=execute(a,b);
    output(sum);

}

